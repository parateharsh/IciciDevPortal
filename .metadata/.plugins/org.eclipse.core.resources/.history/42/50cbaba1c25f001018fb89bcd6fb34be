										responseModel.setMessage("Please upload YAML file");
										responseModel.setStatus(false);
										responseModel.setStatus_code(200);
										return Response.ok(responseModel).build();
									}

									if (uatTLSServiceUrl.equalsIgnoreCase("yes")) {
