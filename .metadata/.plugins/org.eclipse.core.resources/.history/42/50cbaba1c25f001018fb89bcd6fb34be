									LOGGER.info("KVM Success");
									isKVMDone = true;
								}
							} catch (IXCKVMEntryConflictException e) {
								LOGGER.error("KVM already exists INBOUND DMZ");
								isKVMDone = true;
							} catch (Exception e) {
								StringWriter ex = new StringWriter();
								e.printStackTrace(new PrintWriter(ex));
								LOGGER.error(ex.toString());
								isKVMDone = false;
							}
